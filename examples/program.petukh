fn int sum(int a, int b) {
    return (a + b);
}

fn int main() {
    int x = 5, y = 10;

    if (x < y) {
        x = x + sum(3, 7);
    } else if (x == y) {
        x = 0;
    } else {
        x = 1;
    }

    for (int i = 0; i < 3; i = i + 1) {
        x = x + i;
    }

    while (x > 0) {
        x = x - 1;
    }

    do {
        y = y - 1;
    } while (y > 0);

    return x;
}
