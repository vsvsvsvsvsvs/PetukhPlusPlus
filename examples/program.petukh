    int x = 5;
    int y = 10;
    if (x < y) {
        int z = 41;
        x = x + sum(3, 7);
        z = 34;
    } else if (x == y) {
        x = 0;
    } else {
        x = 1;
    }
    for (int i = 0; i < 3; i = i + 1) {
        x = x + i;
        break;
    }

    while (x > 0) {
        x = x - 1;
    }

    do {
        y = y - 1;
    } while (y > 0);

    return x;
}
